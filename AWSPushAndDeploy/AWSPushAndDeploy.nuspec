<?xml version="1.0"?>
<package >
  <metadata>
    <id>AWSPushAndDeploy</id>
    <version>1.5.1</version>
    <title>AWSPushAndDeploy</title>
    <authors>TravCorp</authors>
    <owners>TravCorp</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Wrapper around AWS CodeDeploy API. Code deploys apps and logs output</description>
    <projectUrl>https://github.com/travcorp/aws-tools</projectUrl>
    <releaseNotes></releaseNotes>
    <copyright>Copyright 2015 - TTC</copyright>
    <tags></tags>
    <dependencies>
      <dependency id="CommandLineParser" version="1.9.71" />
      <dependency id="AWSSDK.Core" version="3.1.5.1" />
    </dependencies>
  </metadata>
  <files>
    <!-- including everything since our output is an executable -->
    <file src="bin\Release\*.exe" target="tools" />
    <file src="bin\Release\*.dll" target="tools" />
    <file src="bin\Release\*.config" target="tools" />
  </files>
</package>






